# Environment Variables for Vercel Deployment
# Copy these to your Vercel project's environment variables

# Email Configuration
EMAIL_HOST_PASSWORD=your_gmail_app_password_here

# Debug settings (set to False for production)
DEBUG=False

# Secret key (generate a new one for production)
SECRET_KEY=your_production_secret_key_here

# Allowed hosts (add your Vercel domain)
ALLOWED_HOSTS=localhost,127.0.0.1,your-vercel-domain.vercel.app
